<!--우측 상단 헤더 영역 -->
<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>관리자정보조회</h2>
    <ol class="breadcrumb">
      <li>
        <a href="/Home/Index">Home</a>
      </li>
      <li>
        <a>관리자정보</a>
      </li>
      <li class="active">
        <strong>조회</strong>
      </li>
    </ol>
  </div>
  <div class="col-lg-2"></div>
</div>

<!--우측 콘텐츠 영역 -->
<div class="wrapper wrapper-content">
  <div class="ibox-content m-b-sm border-bottom">
    <div class="row">
      <div class="col-sm-4">
        <div class="form-group">
          <label class="control-label" for="OrderNo">관리자명</label>
          <input type="text" class="form-control" />
        </div>
      </div>
      <div class="col-sm-4">
        <div class="form-group">
          <label class="control-label" for="OrderName">아이디</label>
          <input type="text" class="form-control" />
        </div>
      </div>
      <div class="col-sm-4">
        <div class="form-group">
          <label class="control-label" for="MobileNo">사용여부</label>
          <input type="text" class="form-control" />
        </div>
      </div>
    </div>
    <div class="text-center">
      <button type="button" class="btn btn-primary">조회</button>
      <a href="/admin/entry" class="btn btn-info">신규</a>
    </div>
  </div>

  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-content">
          <div class="table-responsive">
            <table class="table table-hover">
              <thead>
                <tr>
                  <th width="10%">관리번호</th>
                  <th width="10%">이름</th>
                  <th width="10%">아이디</th>
                  <th width="20%">메일</th>
                  <th width="20%">전화번호</th>
                  <th width="10%">사용여부</th>
                  <th width="20%">등록일</th>
                </tr>
              </thead>
              <tbody class="hoverTblBody">
                  <% for(var i =0;i<admin_members.length;i++){ %>
                <tr>
                  <td><a href="/admin/modify?aid=<%= admin_members[i].admin_member_id%>&test=sample"><%= admin_members[i].admin_member_id%></a></td>
                  <td><a href="/admin/modify?aid=<%= admin_members[i].admin_member_id%>&test=sample"><%= admin_members[i].admin_name%></a></td>
                  <td><%= admin_members[i].admin_id%></td>
                  <td><%= admin_members[i].email%></td>
                  <td><%= admin_members[i].telephone%></td>
                  <td><%= admin_members[i].used_yn_code?"Y":"N"%></td> 
                    <!-- (조건) ? 1번 : 2번      조건이 참이면 1번, 거짓이면 2번 (여기서 code는 1아니면 0이 들어오는데 0은 false.)-->
                  <td><%= admin_members[i].reg_date%></td>
                </tr>
            <% } %>

                <!-- <tr>
                  <td><a href="#">code.AdminID</a></td>
                  <td><a href="#">code.AdminName</a></td>
                  <td>code.AdminEmail</td>
                  <td>code.Company</td>
                  <td>code.CompanyPosition</td>
                  <td>code.UseYN</td>
                  <td>Date</td>
                </tr> -->
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
